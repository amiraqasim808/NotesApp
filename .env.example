PUBLIC_KEY_PATH=./config/public.key

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Email (for notifications or auth)
EMAIL_USER=your_email@example.com
EMAIL_PASS=your_email_app_password
